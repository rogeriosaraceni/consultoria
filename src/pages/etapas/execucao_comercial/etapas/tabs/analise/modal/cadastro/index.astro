---
import ContentForm from "./_content-form.astro";
---

<style is:inline>
    .modal.modal-cadastroAnalise{
        --bs-modal-width: 1600px !important;
    }
    .modal-content.modal-content-app{
        --bs-modal-bg: #f0f2f5;
    }

    .modal-content.modal-content-app .card{
        margin-left: 0!important;
        margin-right: 0!important;
    }
</style>

<div class="modal modal-cadastroAnalise fade" id="modalCadAnalise" tabindex="-1" aria-labelledby="modalCadAnaliseLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content modal-content-app">
            <ContentForm />
        </div>
    </div>
</div>
