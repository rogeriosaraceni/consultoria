---
import Layout from '@layouts/Layout.astro';
import Nav from '@includes/Nav/Nav.astro';
import PageHeader from '@components/PageHeader.astro';

import BtnsEtapas from './_content-btns-etapas.astro'
import BarGerenteConsultor from './_content-bar-gerente-consultor.astro'
---

<Layout>
    <Nav />

    <PageHeader
        icon='bi bi-building-gear',
        name='Execução Comercial'
    />

    <main class="main-app">
        <section class="container-fluid">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb breadcrumb p-0 mx-md-4">
                    <li class="breadcrumb-item"><a href="/inicio/">Início</a></li>
                    <li class="breadcrumb-item">Etapas</li>
                    <li class="breadcrumb-item"><a href="/etapas/execucao_comercial/">Execução Comercial</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Execução</li>
                </ol>
            </nav>

            <BarGerenteConsultor />
            <BtnsEtapas />
        </section>
    </main>
</Layout>
